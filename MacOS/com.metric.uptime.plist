<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.metric.uptime</string>
    <key>ProgramArguments</key>
    <array>
        <string>/Users/Shared/uptime.sh</string>
    </array>
    <key>KeepAlive</key>
    <dict>
        <key>OtherUserLogins</key>
        <true/>
    </dict>
    <key>RunAtLoad</key>
    <true/>
    <key>StartOnMount</key>
    <true/>
    <key>StandardErrorPath</key>
    <string>/tmp/com.metric.uptime.err</string>
    <key>StandardOutPath</key>
    <string>/tmp/com.metric.uptime.out</string>
</dict>
</plist>
